{
  "name": "smart-search",
  "version": "0.4.0-alpha.0",
  "description": "Extend Quick Search by using the OpenAI API to allow natural language queries in addition to the standard first-token-to-engine mapping",
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/smart-search.git"
  },
  "author": "Jimmy Zhening Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/smart-search/issues"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/smart-search#readme",
  "scripts": {
    "clean": "npm run clean:dist && npm run clean:lint-cache",
    "clean:dist": "shx rm -rf dist",
    "clean:lint-cache": "shx rm -rf .eslintcache",
    "lint": "eslint --cache --cache-strategy content -c eslint.config.js **",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "npm run lint:fix",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "dev:clean": "npm run clean && npm run build && npm run start"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.4"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "sanitize-filename": "^1.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^8.55.0",
    "@stylistic/eslint-plugin": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "iso-639-1": "^3.1.0",
    "shx": "^0.3.4",
    "typescript": "^5.3.2"
  }
}
