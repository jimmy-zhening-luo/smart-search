{
  "name": "smart-search",
  "version": "0.4.0-alpha.0",
  "description": "Extend Quick Search by using the OpenAI API to allow natural language queries in addition to the standard first-token-to-engine mapping",
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/smart-search.git"
  },
  "author": "Jimmy Zhening Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/smart-search/issues"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/smart-search#readme",
  "scripts": {
    "lint": "eslint --cache --cache-strategy content -c eslint.config.js **",
    "lint:fix": "npm run lint -- --fix",
    "clean": "shx rm -rf dist",
    "build": "tsc",
    "rebuild": "npm run lint && npm run clean && npm run build",
    "start": "node dist/index.js",
    "dev": "npm run rebuild && npm run start",
    "debug": "npm run build && npm run start"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.4"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "openai": "^4.20.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.54.0",
    "@stylistic/eslint-plugin": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "shx": "^0.3.4",
    "typescript": "^5.3.2"
  }
}